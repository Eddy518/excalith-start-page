{
	"username": "edterm",
	"theme": {
		"name": "Nord",
		"backgroundColor": "#2e3440",
		"windowColor": "#3b4252",
		"textColor": "#E5E9F0",
		"black": "#3B4252",
		"red": "#BF616A",
		"green": "#A3BE8C",
		"yellow": "#EBCB8B",
		"blue": "#81A1C1",
		"magenta": "#B48EAD",
		"cyan": "#88C0D0",
		"white": "#E5E9F0",
		"gray": "#d8dee9"
	},
	"wallpaper": {
		"url": "assets/new_moon.png",
		"easing": "ease-in-out",
		"fadeIn": true,
		"blur": false
	},
	"terminal": {
		"fixedHeight": true,
		"windowGlow": true,
		"windowGlowColor": "#291f3325",
		"textGlow": false,
		"opacity": 0,
		"blur": 16
	},
	"prompt": {
		"ctrlC": true,
		"placeholder": "command...",
		"placeholderColor": "blue",
		"userColor": "green",
		"atColor": "gray",
		"hostColor": "magenta",
		"promptColor": "magenta",
		"promptSymbol": "‚ùØ",
		"caretColor": "green",
		"selectionBg": "yellow",
		"selectionFg": "black"
	},
	"fetch": {
		"timeFormat": "HH:mm",
		"dateFormat": "DD/MM/YYYY",
		"titleColor": "yellow",
		"image": "assets/default-icon.svg",
		"data": [
			"Time: {time}",
			"Date: {date}",
			"{seperator}",
			"OS: {osName} {osVersion}",
			"Theme: {theme}",
			"{seperator}",
			"Browser: {browser} {browserVersion}",
			"Engine: {engineName}",
			"{seperator}",
			"{colors}"
		]
	},
	"urlLaunch": {
		"target": "_self",
		"hoverColor": "magenta"
	},
	"search": {
		"default": "https://google.com/search?q=",
		"target": "_self",
		"shortcutRegex": "([A-Za-z0-9]+) (.*)",
		"shortcuts": [
			{
				"alias": "g",
				"name": "Google Search",
				"url": "https://google.com/search?q={}"
			},
			{
				"alias": "d",
				"name": "DuckDuckGo Search",
				"url": "https://duckduckgo.com/?q={}"
			},
			{
				"alias": "b",
				"name": "Brave Search",
				"url": "https://search.brave.com/search?q={}"
			},
			{
				"alias": "gh",
				"name": "Github Search",
				"url": "https://github.com/search?q={}"
			},
			{
				"alias": "s",
				"name": "Stack Overflow Search",
				"url": "https://stackoverflow.com/search?q={}"
			},
			{
				"alias": "r",
				"name": "Subreddit Search",
				"url": "https://reddit.com/r/{}"
			},
			{
				"alias": "rd",
				"name": "Reddit Query Search",
				"url": "https://google.com/search?q={} + site:reddit.com"
			},
			{
				"alias": "w",
				"name": "Wikipedia Search",
				"url": "https://en.wikipedia.org/wiki/{}"
			},
			{
				"alias": "t",
				"name": "The Odin Project Search",
				"url": "https://www.google.com/search?q=site:theodinproject.com+{}"
			},
			{
				"alias": "lch",
				"name": "Localhost Search",
				"url": "http://127.0.0.1:{}/"
			},
			{
				"alias": "yt",
				"name": "Youtube Search",
				"url": "https://www.youtube.com/results?search_query={}"
			},
			{
				"alias": "mdn",
				"name": "MDN Search",
				"url": "https://mdn.io/{}"
			}
		]
	},
	"sections": {
		"list": [
			{
				"title": "Quick Access links",
				"color": "magenta",
				"align": "left",
				"links": [
					{
						"name": "TOP",
						"url": "https://www.theodinproject.com/dashboard",
						"icon": "simple-icons:theodinproject"
					},
					{
						"name": "Codewars",
						"url": "https://www.codewars.com/",
						"icon": "simple-icons:codewars"
					},
					{
						"name": "Exercism",
						"url": "https://exercism.org/dashboard",
						"icon": "simple-icons:exercism"
					},
					{
						"name": "React",
						"url": "https://react.dev/",
						"icon": "simple-icons:react"
					}
				]
			},
			{
				"title": "|",
				"color": "magenta",
				"align": "left",
				"links": [
					{
						"name": "Typescript",
						"url": "https://www.typescriptlang.org/docs/",
						"icon": "simple-icons:typescript"
					},
					{
						"name": "JS Info",
						"url": "https://javascript.info/",
						"icon": "simple-icons:javascript"
					},
					{
						"name": "MDN",
						"url": "https://developer.mozilla.org/en-US/",
						"icon": "simple-icons:mdnwebdocs"
					},
					{
						"name": "Devdocs",
						"url": "https://devdocs.io",
						"icon": "simple-icons:readthedocs"
					}
				]
			},
			{
				"title": "|",
				"color": "magenta",
				"align": "left",
				"links": [
					{
						"name": "Node JS",
						"url": "https://nodejs.org/docs/latest/api/",
						"icon": "simple-icons:nodedotjs"
					},
					{
						"name": "Git book",
						"url": "https://git-scm.com/book/en/v2",
						"icon": "simple-icons:git"
					},
					{
						"name": "GitHub",
						"url": "https://github.com/Eddy518/",
						"icon": "simple-icons:github"
					},
					{
						"name": "Youtube",
						"url": "https://www.youtube.com/",
						"icon": "simple-icons:youtube"
					}
				]
			}
		]
	}
}
